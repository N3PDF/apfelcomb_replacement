   RowBox[{"\[Kappa]F", "*",
    RowBox[{"\[Gamma]", "[", "a", "]"}]}], "+",
   RowBox[{
    RowBox[{"(",
     RowBox[{"1", "/", "2"}], ")"}], "*",
    SuperscriptBox["\[Kappa]F", "2"], "*",
    RowBox[{"(",
     RowBox[{
      SuperscriptBox[
       RowBox[{"\[Gamma]", "[", "a", "]"}], "2"], "+", " ",
      RowBox[{
       RowBox[{"\[Beta]", "[", "a", "]"}], "*",
       RowBox[{
        RowBox[{"\[Gamma]", "'"}], "[", "a", "]"}]}]}], ")"}]}], " ", "+",
   " ",
   SuperscriptBox[
    RowBox[{"O", "[", "a", "]"}], "3"]}]}]], "Input",
 CellChangeTimes->{{3.879505108248102*^9, 3.879505218486896*^9}, {
  3.879505292882111*^9, 3.879505299065934*^9}},
